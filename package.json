{
  "name": "bce-bos-client",
  "productName": "BOS",
  "version": "0.1.4-beta",
  "description": "百度开放云-对象存储BOS-客户端",
  "main": "main.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-register --recursive --require ./test/setup.js test/**/*.spec.js",
    "pretest": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "postinstall": "install-app-deps",
    "test-e2e": "cross-env NODE_ENV=test mocha --compilers js:babel-register --require ./test/setup.js ./test/e2e.js",
    "lint": "eslint app test *.js",
    "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
    "start": "cross-env NODE_ENV=production electron ./static/",
    "start-hot": "cross-env DEBUG=bce-client:*,bce-sdk:* HOT=1 NODE_ENV=development electron -r babel-register -r babel-polyfill ./app/main",
    "hot-server": "node -r babel-register server.js",
    "dev": "concurrently --kill-others \"npm run hot-server\" \"npm run start-hot\"",
    "dist": "build",
    "compile": "npm run build-main && npm run build-renderer",
    "publish": "build --x64 --publish onTagOrDraft",
    "build": "npm run compile && npm run dist",
    "build:win": "npm run compile && build --win",
    "build:linux": "npm run compile && build --linux",
    "build:osx": "npm run compile && build --mac"
  },
  "build": {
    "appId": "com.baidu.bce",
    "category": "public.app-category.tools",
    "dmg": {
      "contents": [
        {
          "x": 400,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "win": {
      "iconUrl": "https://bce.bdstatic.com/portal/product/bos/bos-banner_94db6ad3.png"
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ]
    }
  },
  "directories": {
    "buildResources": "build",
    "output": "dist",
    "app": "static"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mudio/bos"
  },
  "author": {
    "name": "木休大人",
    "email": "job.zhanghao@gmail.com",
    "url": "https://github.com/mudio"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mudio/bos/issues"
  },
  "keywords": [
    "bce",
    "bos"
  ],
  "homepage": "https://github.com/mudio/bos#readme",
  "devDependencies": {
    "asar": "^0.12.2",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-webpack-loaders": "^0.7.1",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "chai": "^3.5.0",
    "concurrently": "^2.2.0",
    "cross-env": "^2.0.0",
    "css-loader": "^0.24.0",
    "del": "^2.2.2",
    "devtron": "^1.3.0",
    "electron": "^1.3.4",
    "electron-builder": "^6.3.1",
    "electron-devtools-installer": "^2.0.1",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-import-resolver-webpack": "^0.5.1",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.2",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "fbjs-scripts": "^0.7.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "jsdom": "^9.4.2",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "mocha": "^3.0.2",
    "node-libs-browser": "^1.0.0",
    "postcss": "^5.0.21",
    "react-addons-test-utils": "^15.3.1",
    "redux-logger": "^2.6.1",
    "sinon": "^1.17.5",
    "spectron": "^3.3.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2",
    "webpack-merge": "0.14.1"
  },
  "dependencies": {
    "async": "^2.0.1",
    "bce-sdk-js": "^0.2.0",
    "classnames": "^2.2.5",
    "configstore": "^2.0.0",
    "css-modules-require-hook": "^4.0.2",
    "debug": "^2.2.0",
    "electron-debug": "^1.0.1",
    "font-awesome": "^4.6.3",
    "keytar": "^3.0.2",
    "q": "^1.4.1",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "source-map-support": "^0.4.2",
    "underscore": "^1.8.3"
  },
  "devEngines": {
    "node": "5.x || 6.x",
    "npm": "3.x"
  }
}
